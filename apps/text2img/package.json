{
  "name": "@cdlab996/text2img",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "description": "免费的在线AI文生图工具，基于 Cloudflare Workers AI，支持 FLUX.1、Stable Diffusion XL 等多种模型。无需注册，即刻使用，快速生成高质量AI图像。",
  "author": "wudi <wuchendi96@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/WuChenDi",
  "repository": {
    "type": "git",
    "url": "https://github.com/WuChenDi/projects.git",
    "directory": "apps/text2img"
  },
  "scripts": {
    "dev": "next dev --hostname 0.0.0.0 --port 3012",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "upload": "opennextjs-cloudflare build && opennextjs-cloudflare upload",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts"
  },
  "dependencies": {
    "@cdlab996/genid": "catalog:prod",
    "@cdlab996/ui": "workspace:*",
    "@cdlab996/utils": "workspace:*",
    "@tanstack/react-query": "^5.90.21",
    "class-variance-authority": "catalog:prod",
    "clsx": "catalog:prod",
    "date-fns": "catalog:prod",
    "lucide-react": "catalog:prod",
    "next": "catalog:prod",
    "next-themes": "catalog:prod",
    "react": "catalog:prod",
    "react-dom": "catalog:prod",
    "sonner": "catalog:prod"
  },
  "devDependencies": {
    "@cdlab996/tsconfig": "workspace:*",
    "@opennextjs/cloudflare": "^1.17.1",
    "@types/node": "catalog:dev",
    "@types/react": "catalog:dev",
    "@types/react-dom": "catalog:dev",
    "typescript": "catalog:dev",
    "wrangler": "^4.68.1"
  }
}
